{
  "name": "scss-starter-kit",
  "version": "0.1.0-beta",
  "description": "SCSS starter kit for new projects",
  "private": true,
  "scripts": {
    "start": "webpack --config webpack/webpack.dev.js --watch --progress",
    "build": "npx browserslist && webpack --config webpack/webpack.prod.js --progress --profile --json=dist/stats.json",
    "dev": "webpack-dev-server --config webpack/webpack.dev.js --open --progress",
    "bundle-analyzer": "NODE_ENV=production webpack --config webpack/webpack.analyzer.js",
    "sass:watch": "sass --watch scss/style.scss:dist/assets/style.css",
    "lint:js": "eslint index.js",
    "lint:js:fix": "eslint index.js --fix",
    "lint:scss": "stylelint src/scss/**/*.scss",
    "lint:scss:fix": "stylelint src/scss/**/*.scss --fix"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.7",
    "@babel/core": "^7.25.7",
    "@babel/preset-env": "^7.26.9",
    "@eslint/js": "^9.37.0",
    "babel-loader": "^10.0.0",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "eslint": "^9.37.0",
    "glob-all": "^3.3.1",
    "globals": "^16.4.0",
    "html-inline-css-webpack-plugin": "^1.11.2",
    "html-loader": "^5.0.0",
    "html-webpack-plugin": "^5.6.0",
    "image-minimizer-webpack-plugin": "^4.1.4",
    "imagemin": "^9.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^8.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-pngquant": "^10.0.0",
    "imagemin-svgo": "^11.0.1",
    "imagemin-webp": "^8.0.0",
    "mini-css-extract-plugin": "^2.9.0",
    "postcss": "^8.4.41",
    "postcss-loader": "^8.1.1",
    "postcss-nested": "^7.0.2",
    "postcss-preset-env": "^10.1.5",
    "postcss-sort-media-queries": "^5.2.0",
    "prettier": "^3.6.2",
    "purgecss-webpack-plugin": "^7.0.2",
    "sass": "^1.86.3",
    "sass-loader": "^16.0.5",
    "sharp": "^0.34.4",
    "style-loader": "^4.0.0",
    "stylelint": "^16.23.1",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.92.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^6.0.1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "browserslist": "defaults",
  "dependencies": {
    "@splidejs/splide": "^4.1.4",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21"
  }
}
